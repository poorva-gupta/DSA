class Solution:
    def findKDistantIndices(self, nums: List[int], key: int, k: int) -> List[int]:
        stack=[]
        for i in range(len(nums)):
            for j in range(len(nums)):
                if abs(i-j)<=k and nums[j]==key:
                    if i not in stack:
                        stack.append(i)
                    break
        
        return stack


        

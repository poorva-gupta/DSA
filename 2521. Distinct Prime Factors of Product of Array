class Solution:
    def distinctPrimeFactors(self, nums: List[int]) -> int:
        a=set()
        for num in nums:
            i=2
            while i*i<=num:
                if num%i==0:
                    a.add(i)
                    num//=i
                else:
                    i+=1
            if num>1:
                a.add(num)
        return len(a)

        
